######################################################################
# RADOS performance testing with different iodepths.
#
# NOTE: Make sure you have either Ceph pool named 'rados' or change
#       the pool parameter.
######################################################################
[global]
include iodepth.fio
iodepth=2048

[4k-2048iodepth]
rw=write
bs=4k
size=128k
stonewall

[4k-2048iodepth]
rw=read
bs=4k
size=128k
stonewall

[16k-2048iodepth]
rw=write
bs=16k
size=512k
stonewall

[16k-2048iodepth]
rw=read
bs=16k
size=512k
stonewall

[64k-2048iodepth]
rw=write
bs=64k
size=2m
stonewall

[64k-2048iodepth]
rw=read
bs=64k
size=2m
stonewall

[256k-2048iodepth]
rw=write
bs=256k
size=8m
stonewall

[256k-2048iodepth]
rw=read
bs=256k
size=8m
stonewall

[1m-2048iodepth]
rw=write
bs=1m
size=32m
stonewall

[1m-2048iodepth]
rw=read
bs=1m
size=32m
stonewall

[4m-2048iodepth]
rw=write
bs=4m
size=128m
stonewall

[4m-2048iodepth]
rw=read
bs=4m
size=128m
stonewall
