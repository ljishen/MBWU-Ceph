######################################################################
# RADOS performance testing with different object sizes.
#
# NOTE: Make sure you have either Ceph pool named 'rados' or change
#       the pool parameter.
######################################################################
[global]
include numjobs.fio
numjobs=1
rw=write

[4k-write-1job]
bs=4k
size=128k
stonewall

[16k-write-1job]
bs=16k
size=512k
stonewall

[64k-write-1job]
bs=64k
size=2m
stonewall

[256k-write-1job]
bs=256k
size=8m
stonewall

[1m-write-1job]
bs=1m
size=32m
stonewall

[4m-write-1job]
bs=4m
size=128m
stonewall
