######################################################################
# RADOS performance testing with different object sizes.
#
# NOTE:
# 1. Make sure you have either Ceph pool named 'rados' or change
#    the pool parameter.
# 2. Make sure the open files of user limits is at least 50176 or run
#    $ ulimit -Sn 50176
#    for the session of the fio process.
######################################################################
[global]
include numjobs.fio
numjobs=256

[4k-256jobs]
rw=write
bs=4k
size=128k
stonewall

[4k-256jobs]
rw=read
bs=4k
size=128k
stonewall

[16k-256jobs]
rw=write
bs=16k
size=512k
stonewall

[16k-256jobs]
rw=read
bs=16k
size=512k
stonewall

[64k-256jobs]
rw=write
bs=64k
size=2m
stonewall

[64k-256jobs]
rw=read
bs=64k
size=2m
stonewall

[256k-256jobs]
rw=write
bs=256k
size=8m
stonewall

[256k-256jobs]
rw=read
bs=256k
size=8m
stonewall

[1m-256jobs]
rw=write
bs=1m
size=32m
stonewall

[1m-256jobs]
rw=read
bs=1m
size=32m
stonewall

[4m-256jobs]
rw=write
bs=4m
size=128m
stonewall

[4m-256jobs]
rw=read
bs=4m
size=128m
stonewall
